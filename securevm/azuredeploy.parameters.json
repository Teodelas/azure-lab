{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "aadClientID": {
      "reference": {
        "keyVault": {
          "id": "[variables('keyVaultResourceID')]"
        },
        "secretName": "aadclientid"
      }
    },
    "aadClientSecret": {
      "reference": {
        "keyVault": {
          "id": "[variables('keyVaultResourceID')]"
        },
        "secretName": "aadclientsecret"
      }
    },
    "vmName": {
      "value": "securevm"
      }
    
  }
}
